    "from datetime import datetime, timedelta\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "import numpy as np\n",
    "\n",
    "Path(\"images\").mkdir(exist_ok=True)\n",
    "\n",
    "def parse_goes_time_from_key(key):\n",
    "    fname = key.split(\"/\")[-1]\n",
    "    s_index = fname.find(\"_s\")\n",
    "    e_index = fname.find(\"_e\")\n",
    "    timestr = fname[s_index+2:e_index]  # YYYYJJJHHMMSS\n",
    "    year = int(timestr[0:4])\n",
    "    jday = int(timestr[4:7])\n",
    "    hour = int(timestr[7:9])\n",
    "    minute = int(timestr[9:11])\n",
    "    second = int(timestr[11:13])\n",
    "    dt = datetime(year, 1, 1) + timedelta(days=jday-1, hours=hour, minutes=minute, seconds=second)\n",
    "    return dt\n",
    "\n",
    "for frame_index, key in enumerate(selected_files):\n",
    "    print(\"Redrawing PNG with coastlines for frame\", frame_index, \":\", key)\n",
    "\n",
    "    with fs.open(key, mode=\"rb\") as f:\n",
    "        try:\n",
    "            ds = xr.open_dataset(f, engine=\"h5netcdf\")\n",
    "        except Exception as e:\n",
    "            print(\"h5netcdf failed, trying netcdf4:\", e)\n",
    "            f.seek(0)\n",
    "            ds = xr.open_dataset(f, engine=\"netcdf4\")\n",
    "        ds.load()\n",
    "\n",
    "    CMI = ds[\"CMI\"]\n",
    "    x = ds[\"x\"]\n",
    "    y = ds[\"y\"]\n",
    "\n",
    "    # Downsample to same grid you used for CSVs\n",
    "    CMI_small = CMI.coarsen(y=fac_y, x=fac_x, boundary=\"trim\").mean()\n",
    "    arr_small = CMI_small.values\n",
    "\n",
    "    x_small = x.coarsen(x=fac_x, boundary=\"trim\").mean().values\n",
    "    y_small = y.coarsen(y=fac_y, boundary=\"trim\").mean().values\n",
    "\n",
    "    vmin = np.nanpercentile(arr_small, 2)\n",
    "    vmax = np.nanpercentile(arr_small, 98)\n",
    "\n",
    "    fig = plt.figure(figsize=(6, 6), dpi=150)\n",
    "    ax = plt.axes(projection=goes_crs)\n",
    "\n",
    "    X, Y = np.meshgrid(x_small, y_small)\n",
    "\n",
    "    mesh = ax.pcolormesh(\n",
    "        X, Y, arr_small,\n",
    "        transform=goes_crs,\n",
    "        cmap=\"inferno\",\n",
    "        vmin=vmin,\n",
    "        vmax=vmax\n",
    "    )\n",
    "\n",
    "    ax.coastlines(resolution=\"110m\", color=\"black\", linewidth=1.2)\n",
    "    ax.set_global()\n",
    "    #ax.outline_patch.set_visible(False)\n",
    "    ax.set_axis_off()\n",
    "\n",
    "    # Use the SAME filenames you already used for your images\n",
    "    img_name = f\"goes_c13_frame_{frame_index:02d}.png\"\n",
    "    img_path = Path(\"images\") / img_name\n",
    "    fig.savefig(img_path, bbox_inches=\"tight\", pad_inches=0, facecolor=\"black\")\n",
    "    plt.close(fig)\n",
    "\n",
    "print(\"Done: images overwritten with coastline-enhanced PNGs.\")\n"
