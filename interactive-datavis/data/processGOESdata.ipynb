    "    semiminor_axis=float(proj_info.semi_minor_axis)\n",
    ")\n",
    "\n",
    "goes_crs = ccrs.Geostationary(\n",
    "    central_longitude=central_lon,\n",
    "    satellite_height=sat_height,\n",
    "    sweep_axis=sweep_axis,\n",
    "    globe=globe\n",
    ")\n",
    "\n",
